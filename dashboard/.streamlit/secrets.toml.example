# Streamlit Cloud Secrets Configuration
# Copy this file to secrets.toml and fill in your Snowflake credentials

# Format required for st.connection("snowflake", type="sql")
[connections.snowflake]
account = "your-account-identifier"
user = "your-username"
password = "your-password"
warehouse = "COMPUTE_WH"
database = "FRAUDLENS"
schema = "GOLD"
